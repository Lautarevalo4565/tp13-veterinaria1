package com.mycompany.tp13veterinaria;

/**
 *
 * @author Lautaro
 */
public class Cat extends Animales {
    
    private String raza;
    
    public Cat(String nombre, String raza, int edad){
        super(nombre, edad);
        this.raza = raza;
    }
    
    @Override
    public void hacerSonido(){
        System.out.println (nombre + " miau ");
    }
    
    @Override
    public void MostrarInfo(){
        super.MostrarInfo();
        System.out.println ("Raza: " + raza);
    }
    
     public void araniar(){
        System.out.println("El gato arania");
    }

    
}
