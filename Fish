package com.mycompany.tp13veterinaria;

/**
 *
 * @author Lautaro
 */
public class Fish extends Animales {
    
    private String especie;
    
    public Fish(String nombre, String especie, int edad){
        super(nombre, edad);
        this.especie = especie;
    }
    
    @Override
    public void hacerSonido(){
        System.out.println (GetNombre + " glu glu ");
    }
    
    @Override
    public void MostrarInfo(){
        super.MostrarInfo();
        System.out.println ("Especie: " + especie);
    }
    
     public void nadar(){
        System.out.println("El pez nada");
    }

    
}
