package com.mycompany.tp13veterinaria;

/**
 *
 * @author Lautaro
 */
public class Dog extends Animales {
    
    private String raza;
    
    public Dog(String nombre, String raza, int edad){
        super(nombre, edad);
        this.raza = raza;
    }
    
    @Override
    public void hacerSonido(){
        System.out.println (GetNombre + " guau guau ");
    }
    
    @Override
    public void MostrarInfo(){
        super.MostrarInfo();
        System.out.println ("Raza: " + raza);
    }
    
     public void correr(){
        System.out.println("El perro corre");
    }

    
}
